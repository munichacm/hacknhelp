{
  "name": "wct-local",
  "version": "1.7.0",
  "description": "WCT plugin that enables support for local browsers via Selenium",
  "keywords": [
    "wct",
    "web-component-tester",
    "plugin"
  ],
  "homepage": "https://github.com/Polymer/wct-local",
  "bugs": {
    "url": "https://github.com/Polymer/wct-local/issues"
  },
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Polymer/wct-local.git"
  },
  "main": "lib/plugin.js",
  "scripts": {
    "postinstall": "node scripts/postinstall.js"
  },
  "wct-plugin": {
    "cli-options": {
      "browsers": {
        "help": "Local browsers to run tests on, or 'all'.",
        "full": "local",
        "metavar": "NAME",
        "abbr": "l",
        "list": true
      },
      "seleniumPort": {
        "help": "The port of a running selenium server to use.",
        "full": "selenium-port"
      },
      "skipSeleniumInstall": {
        "help": "Skip trying to install selenium.",
        "full": "skip-selenium-install",
        "flag": true
      }
    }
  },
  "devDependencies": {
    "gulp": "^3.8.10",
    "gulp-jshint": "^1.9.0",
    "jshint-stylish": "^2.0.1"
  },
  "dependencies": {
    "chalk": "^1.1.1",
    "cleankill": "^1.0.0",
    "freeport": "^1.0.4",
    "launchpad": "^0.4.4",
    "lodash": "^3.0.1",
    "selenium-standalone": "^4.6.1",
    "which": "^1.0.8"
  },
